{
  "name": "express-scaffold",
  "version": "1.0.0",
  "description": "This is Express Scaffold.",
  "main": "index.js",
  "repository": "git@github.com:kojiyamauchi/express-scaffold.git",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@4.6.0",
  "engines": {
    "npm": "use yarn instead.",
    "yarn": "4.6.0"
  },
  "workspaces": {
    "packages": [
      "./apps",
      "./client"
    ],
    "nohoist": []
  },
  "devDependencies": {
    "cspell": "^9.2.0",
    "husky": "9.1.7",
    "lint-staged": "^16.1.4",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "setup": "rm -rf .git",
    "prepare": "husky install",
    "dev:apps": "yarn workspace apps dev",
    "dev:client": "yarn workspace client dev",
    "build:apps": "yarn workspace apps build",
    "build:client": "yarn workspace client build",
    "clean:apps": "yarn workspace apps clean",
    "clean:client": "yarn workspace client clean",
    "lint:apps": "yarn workspace apps lint",
    "lint:client": "yarn workspace client lint",
    "stylelint": "yarn workspace client stylelint",
    "typecheck:apps": "yarn workspace apps tsc --noEmit",
    "typecheck:client": "yarn workspace client tsc --noEmit",
    "jest:apps": "yarn workspace apps jest",
    "jest:client": "yarn workspace client jest",
    "spellcheck": "yarn cspell --dot --no-must-find-files '**/*'",
    "dev": "run-p \"dev:*\"",
    "build": "run-p \"build:*\"",
    "clean": "run-p \"clean:*\"",
    "serve": "yarn workspace apps serve",
    "serve:local": "yarn workspace apps serve:local",
    "ui": "yarn upgrade-interactive",
    "prisma:init": "yarn workspace apps prisma:init",
    "prisma:generate": "yarn workspace apps prisma:generate",
    "prisma:migrate:dev": "yarn workspace apps prisma:migrate:dev",
    "prisma:migrate:reset": "yarn workspace apps prisma:migrate:reset",
    "prisma:migrate:deploy": "yarn workspace apps prisma:migrate:deploy",
    "prisma:migrate:status": "yarn workspace apps prisma:migrate:status",
    "prisma:push": "yarn workspace apps prisma:push",
    "prisma:pull": "yarn workspace apps prisma:pull",
    "prisma:seed": "yarn workspace apps prisma:seed",
    "prisma:validate": "yarn workspace apps prisma:validate",
    "prisma:format": "yarn workspace apps prisma:format",
    "prisma:studio": "yarn workspace apps prisma:studio"
  }
}
